        <control type="group">
            <include>openclose</include>
            <include>opencloseslideup</include>
            <left>0</left>
            <top>897</top>
			<height>40</height>
            <ondown>9000</ondown>
            <control type="group">
                <left>-15</left>
                <top>-129</top>
                <animation type="Conditional" condition="ControlGroup(9001).HasFocus()">
                    <effect type="fade" start="0" end="100" time="200" />
                </animation>
                <animation type="Conditional" condition="!ControlGroup(9001).HasFocus()">
                    <effect type="slide" start="0,-30" end="0,0" time="200" tween="quadratic" easing="out" />
                    <effect type="fade" start="100" end="0" time="200" />
                </animation>
