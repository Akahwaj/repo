<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>11</defaultcontrol>
	<zorder>2</zorder>
	<controls>
		<control type="progress" id="32">
			<include>HiddenObject</include>
		</control>
		<!-- Dialog -->
		<control type="group">
			<centertop>50%</centertop>
			<centerleft>50%</centerleft>
			<include>Dialog_PopupOpenClose</include>
			<width>500</width>
			<height>145</height>
			<!-- Background image -->
			<control type="image">
				<texture colordiffuse="panel" border="10">common/dialog_back.png</texture>
			</control>
			<!-- Heading -->
			<control id="10" type="label">
				<top>15</top>
				<left>20</left>
				<right>20</right>
				<font>Med24</font>
				<align>center</align>
			</control>
			<!-- Details -->
			<control id="12" type="label">
				<bottom>15</bottom>
				<height>25</height>
				<left>20</left>
				<right>20</right>				
				<font>Reg22</font>
				<align>center</align>
				<scroll>true</scroll>
			</control>
			<!-- Slider -->
			<control type="group">
				<centertop>50%</centertop>
				<left>20</left>
				<right>20</right>
				<height>26</height>
				<control type="image">
					<top>8</top>
					<description>Slider</description>
					<width>100%</width>
					<height>10</height>
					<texture colordiffuse="background_diffuse">common/white.jpg</texture>
				</control>
				<control type="slider" id="11">
					<width>100%</width>
					<height>8</height>
					<onup>11</onup>
					<ondown>11</ondown>
					<onleft>11</onleft>
					<onright>11</onright>
				</control>
			</control>
		</control>
	</controls>
</window>