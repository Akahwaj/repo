<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>All Movies</name>
    <match>all</match>
	<rule field="filename" operator="doesnotcontain">
        <value>.SEX.</value>
        <value>SEX</value>
		<value>.UHD.</value>
		<value>UHD</value>
    </rule>
    <rule field="genre" operator="doesnotcontain">
        <value>SEX</value>
		<value>UHD</value>
    </rule>
	<order direction="ascending">sorttitle</order>
</smartplaylist>